URxvt.depth:		32
URxvt.scrollBar:	false
URxvt.cursorColor:	white
URxvt.font:     xft:bitstream vera sans mono:size=14:antialias=true,xft:Inconsolata\ for\ Powerline:size=14, xft:xos4 
Terminus:pixelsize=14, xft:xos4 Terminess Powerline:pixelsize=14
URxvt.boldFont: xft:bitstream vera sans mono:bold:size=14:antialias=true, xtf:xos4 Terminus:pixelsize=14:weight=bold, xft:xos4 Terminess Powerline:pixelsize=14
URxvt.perl-ext-common:	default,matcher,resize-font,clipboard,keyboard-select
! keyboard-select
URxvt.keysym.M-s: perl:keyboard-select:activate
URxvt.keysym.M-r: perl:keyboard-select:search
! clipboard
! update clipboard whenever primary gets updated
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.url-launcher:	/usr/bin/xdg-open
URxvt.matcher.button:	1
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.resize-font.reset: C-Left
URxvt.resize-font.show: C-Right

! special
*.foreground:   #ffffff
*.background:   [90]#1b1b1b
*.cursorColor:  #ffffff

! black
*.color0:       #3d3d3d
*.color8:       #4d4d4d

! red
*.color1:       #6673bf
*.color9:       #899aff

! green
*.color2:       #3ea290
*.color10:      #52ad91

! yellow
*.color3:       #b0ead9
*.color11:      #98c9bb

! blue
*.color4:       #31658c
*.color12:      #477ab3

! magenta
*.color5:       #596196
*.color13:      #7882bf

! cyan
*.color6:       #8292b2
*.color14:      #95a7cc

! white
*.color7:       #c8cacc
*.color15:      #edeff2

